# Finance Agent Environment Configuration - Boardroom TEE

# Development mode
DEVELOPMENT_MODE=true
MOCK_TEE_ATTESTATION=true  
MOCK_LLM_PROCESSING=true

# Agent configuration
AGENT_TYPE=finance
AGENT_MODEL_NAME=AdaptLLM/finance-LLM
AGENT_API_PORT=8081
AGENT_ATTESTATION_PORT=29344
AGENT_MAX_MEMORY_MB=7000
AGENT_HOST=0.0.0.0

# Client identification
CLIENT_ID=your-company-name

# Hub connection (configure during deployment)
HUB_ENDPOINT=http://localhost:8080
HUB_ATTESTATION_ENDPOINT=http://localhost:29343

# Logging
LOG_LEVEL=INFO

# TEE Security
TEE_PLATFORM=secretvm
ATTESTATION_REQUIRED=true